<!-- 喜欢的影单 -->
<view class='container page-favlist'>
  <block wx:for='{{list}}' wx:key='id'>
    <navigator url='/pages/article/detail?id={{1074}}' hover-class='none'>
      <com-article checked aid='{{item.id}}' image='{{item.image}}' title='{{item.title}}' like-count='{{item.likeCount}}' bindchange='onFavChange' data-index='{{index}}' />
    </navigator>
  </block>
</view>